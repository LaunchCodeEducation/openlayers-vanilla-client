{
  "name": "openlayers-project",
  "version": "1.0.0",
  "description": "Starter files for building a standalone OpenLayers client using otherwise vanilla JavaScript. Does not require webpack or any other bundling tool. Only uses native JavaScript modules to simplify development. Provides intellisense type completion in VSCode for the OpenLayers library.",
  "main": "src",
  "scripts": {
    "lint": "eslint --fix .",
    "format": "prettier --write 'src/*' '!src/openlayers/*'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LaunchCodeEducation/openlayers-vanilla-client.git"
  },
  "author": "",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/LaunchCodeEducation/openlayers-vanilla-client/issues"
  },
  "homepage": "https://github.com/LaunchCodeEducation/openlayers-vanilla-client#readme",
  "devDependencies": {
    "@types/openlayers": "^4.6.17",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1"
  },
  "prettier": {},
  "eslintConfig": {
    "plugins": [
      "prettier"
    ],
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "env": {
      "node": true,
      "es6": true,
      "browser": true
    },
    "ignorePatterns": [
      "src/openlayers/"
    ],
    "rules": {
      "import/extensions": 0
    }
  }
}
